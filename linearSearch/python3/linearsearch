def search(arr, x, n):
    for i in range(0, n):
        if arr[i] == x:
            return i

    return -1


arr = list()
n = int(input("enter list size:"))
for i in range(0, n):
    t = int(input("Enter next nUmber: "))
    arr.append(t)

print(arr)
x = int(input("enter element to search:"))
result = search(arr, x, n)
if result == -1:
    print("Element is not found")
else:
    print("Element is at index: ", result)

